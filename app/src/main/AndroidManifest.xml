<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.notadeveloper.app.pat">

  <uses-permission android:name="android.permission.INTERNET"/>
  <application
      android:name="android.support.multidex.MultiDexApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      tools:replace="android:supportsRtl">
    <activity
        android:name="com.notadeveloper.app.pat.Login"
        android:theme="@style/AppTheme.NoActionBar">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name="com.notadeveloper.app.pat.Register"/>
    <activity
        android:name="com.notadeveloper.app.pat.MainActivity"
        android:theme="@style/AppThemenotool"/>
    <activity android:name="com.notadeveloper.app.pat.detailedpartyactivity"/>
    <activity android:name="com.notadeveloper.app.pat.DetailedTicketActivity"/>
    <activity android:name="com.notadeveloper.app.pat.MyTickets"/>
    <activity android:name="com.notadeveloper.app.pat.SheeshaActivity"/>
    <activity android:name="com.notadeveloper.app.pat.SheeshaOrderActivity"/>
    <activity android:name="com.notadeveloper.app.pat.ClubDetailedActivity"/>
  </application>

</manifest>